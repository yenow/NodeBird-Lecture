{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["plugin:react/recommended","eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "react/react-in-jsx-scope" : "warn",
    "prettier/prettier": "warn",
    "no-unused-vars" : "warn"
  }
}
